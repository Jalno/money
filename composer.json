{
    "name": "jalno/money",
    "description": "Money and currency library",
    "type": "library",
    "keywords": [
        "Jalno",
        "Money",
        "Currency"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-gmp": "*",
        "brick/math": "^0.9.2"
    },
    "require-dev": {
        "ext-bcmath": "*",
        "ext-gmp": "*",
        "phpstan/phpstan": "^0.12.92"
    },
    "autoload": {
        "psr-4": {
            "Jalno\\Money\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jalno\\Money\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
